{
  "schemaVersion": 1,
  "id": "starscript-modules",
  "version": "${version}",
  "name": "Starscript Modules",
  "description": "An addon that adds empty modules to be used with Starscript",
  "authors": [
    "IvanProCookie",
    "OrangeMan432"
  ],
  "contact": {
    "repo": "https://github.com/IvanProCookie/Modules-Plus-Meteor-Addon"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.ivanprocookie.addon.StarscriptModules"
    ]
  },
  "mixins": [
    "starscript-modules.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
